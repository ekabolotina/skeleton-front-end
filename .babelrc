{
    "env": {
        "development": {
            "presets": ["next/babel"],
            "plugins": [
                [
                    "@babel/plugin-proposal-decorators",
                    { "legacy": true }
                ],
                [
                    "@babel/plugin-proposal-class-properties",
                    { "loose": false }
                ],
                "@emotion"
            ]
        },
        "production": {
            "presets": ["next/babel"],
            "plugins": [
                [
                    "@babel/plugin-proposal-decorators",
                    { "legacy": true }
                ],
                [
                    "@babel/plugin-proposal-class-properties",
                    { "loose": false }
                ],
                "@emotion"
            ]
        }
    }
}
